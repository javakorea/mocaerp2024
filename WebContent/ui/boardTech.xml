                			<w2:column width="50" inputType="text" id="column9" value="글번호" displayMode="label"></w2:column>
                			<w2:column width="200" inputType="text" id="column15" value="제목" displayMode="label"></w2:column>
                			<w2:column width="20" inputType="text" style="" id="H_CNT" value="첨부" displayMode="label"></w2:column>
                			<w2:column width="50" inputType="text" id="column16" value="작성자" displayMode="label"></w2:column>
                			<w2:column width="80" inputType="text" id="column17" value="작성일시" displayMode="label"></w2:column>
                			<w2:column width="80" inputType="text" id="column18" value="수정일시" displayMode="label"></w2:column>
                		</w2:row>

                	</w2:header>

                	<w2:gBody id="gBody2" style="">

                		<w2:row id="row4" style="">
                			<w2:column width="30" inputType="checkbox" style="" id="CHK" value="" displayMode="label" readOnly="false" falseValue="0" trueValue="1" defaultValue="0" valueType="binary"></w2:column>
                			<w2:column width="50" inputType="text" id="BOARD_IDX" displayMode="label" readOnly="true"></w2:column>
                			<w2:column width="200" inputType="text" id="BOARD_TITLE" displayMode="label" textAlign="left" readOnly="true"></w2:column>
                			<w2:column width="20" inputType="text" style="" id="CNT" value="" displayMode="label" readOnly="true"></w2:column>
                			<w2:column width="50" inputType="text" id="BOARD_WRITER" displayMode="label" readOnly="true"></w2:column>
                			<w2:column width="80" inputType="text" id="BOARD_DATE" displayMode="label" readOnly="true" dataType="text" value=""></w2:column>
                			<w2:column width="80" inputType="text" id="UPDATE_DATE" displayMode="label" dataType="date" readOnly="true" value=""></w2:column>
                		</w2:row>

                	</w2:gBody>

                </w2:gridView>

            </xf:group>
                	
        </xf:group>
           
    </body>
    
</html>
